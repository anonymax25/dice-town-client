/*!*********************************************************!*\
  !*** ./src/app/components/lobby/game/game.component.ts ***!
  \*********************************************************/
/*! exports provided: GameComponent */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "GameComponent", function() { return GameComponent; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "mrSG");
/* harmony import */ var _raw_loader_game_component_html__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! raw-loader!./game.component.html */ "jz00");
/* harmony import */ var _game_component_scss__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! ./game.component.scss */ "1cfz");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! @angular/core */ "fXoL");
/* harmony import */ var _angular_material_dialog__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! @angular/material/dialog */ "0IaG");
/* harmony import */ var src_app_models_dice__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! src/app/models/dice */ "e/1D");
/* harmony import */ var src_app_models_game_status_label_enum__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! src/app/models/game-status-label.enum */ "4hnz");
/* harmony import */ var src_app_models_game_status_enum__WEBPACK_IMPORTED_MODULE_7__ = __webpack_require__(/*! src/app/models/game-status.enum */ "ZpNx");
/* harmony import */ var src_app_services_authentication_service__WEBPACK_IMPORTED_MODULE_8__ = __webpack_require__(/*! src/app/services/authentication.service */ "ej43");
/* harmony import */ var src_app_shared_snackbar_snackbar_service__WEBPACK_IMPORTED_MODULE_9__ = __webpack_require__(/*! src/app/shared/snackbar/snackbar.service */ "zkTT");
/* harmony import */ var _layout_confirm_confirm_component__WEBPACK_IMPORTED_MODULE_10__ = __webpack_require__(/*! ../../layout/confirm/confirm.component */ "fGbU");
/* harmony import */ var _lobby_socket_service__WEBPACK_IMPORTED_MODULE_11__ = __webpack_require__(/*! ../lobby-socket.service */ "p5Eu");












let GameComponent = class GameComponent {
    constructor(authenticationService, lobbySocketService, snackbarService, dialog) {
        this.authenticationService = authenticationService;
        this.lobbySocketService = lobbySocketService;
        this.snackbarService = snackbarService;
        this.dialog = dialog;
        this.updateGameEvent = new _angular_core__WEBPACK_IMPORTED_MODULE_3__["EventEmitter"]();
        this.gameStatusLabel = src_app_models_game_status_label_enum__WEBPACK_IMPORTED_MODULE_6__["GameStatusLabel"];
        this.gameStatus = src_app_models_game_status_enum__WEBPACK_IMPORTED_MODULE_7__["GameStatus"];
        this.newDices = [];
        this.selectedDice = [];
        this.costs = 0;
        this.rollingDices = false;
        this.isDiceChosen = false;
        this.isCostError = false;
    }
    ngOnInit() {
        this.lobbySocketService.updateGame().subscribe(game => {
            this.updateGameEvent.emit(game);
        });
    }
    getFirst3properties() {
        return [...this.lobby.game.property].slice(0, 3);
    }
    getSherif() {
        return this.lobby.users.find(user => this.lobby.game.sherifUserid === user.id);
    }
    getPlayer() {
        return this.lobby.game.players.find(player => player.userId === this.authenticationService.getIdFromToken());
    }
    getGameStatus() {
        return src_app_models_game_status_label_enum__WEBPACK_IMPORTED_MODULE_6__["GameStatusLabel"][this.lobby.game.status];
    }
    rollDices() {
        this.lobby.game.players.find(player => player.userId === this.authenticationService.getIdFromToken()).canThrowDices = false;
        this.isDiceChosen = false;
        let player = this.getPlayer();
        const newDicesLen = 5 - player.dices.length;
        const isAutoChooseAll = this.lobby.game.players.some(player => this.nonHiddenDices(player.dices).length === 5);
        this.setRandomDices(newDicesLen, isAutoChooseAll);
    }
    validateDiceChoice() {
        this.isCostError = false;
        if (this.costs >= this.getPlayer().dollar) {
            this.snackbarService.openError(`Not enough money: ${this.getPlayer().dollar}$ - ${this.costs}$ = ${this.getPlayer().dollar - this.costs}$`);
            this.isCostError = true;
            return;
        }
        this.dialog.open(_layout_confirm_confirm_component__WEBPACK_IMPORTED_MODULE_10__["ConfirmComponent"], {
            height: '200px',
            width: '500px',
        }).afterClosed().subscribe(doAction => {
            if (doAction) {
                this.lobbySocketService.setDices(this.lobby.id, this.newDices.filter((dice, index) => this.isDiceSelected(index)));
                this.isDiceChosen = true;
                this.selectedDice = [];
                this.newDices = [];
            }
        });
    }
    setRandomDices(count, isAutoChooseAll = false) {
        this.rollingDices = true;
        setTimeout(() => {
            this.rollingDices = false;
            let dices = [];
            for (let i = 0; i < count; i++) {
                dices.push(new src_app_models_dice__WEBPACK_IMPORTED_MODULE_5__["Dice"](this.getRandomInt(9, 14), true));
            }
            this.newDices = dices;
            if (isAutoChooseAll) {
                //if at least one player has 5 dices all dices are selected, and no more costs added
                this.selectedDice = [];
                for (let i = 0; i < this.newDices.length; i++) {
                    this.selectDice(i, true);
                }
                this.costs = 0;
            }
            else {
                this.selectDice(0, true);
            }
        }, 1000);
    }
    getRandomInt(min, max) {
        min = Math.ceil(min);
        max = Math.floor(max);
        return Math.floor(Math.random() * (max - min + 1)) + min;
    }
    selectDice(index, isAutoSelect = false) {
        if (!isAutoSelect && this.lobby.game.players.some(player => this.nonHiddenDices(player.dices).length === 5)) {
            this.snackbarService.openError("Can't un-select dices, this is the last round (an other player has reached 5 dices)");
            return;
        }
        if (index < 0) {
            this.selectedDice = [];
        }
        else {
            if (!this.isDiceSelected(index))
                this.selectedDice.push(index);
            else
                this.selectedDice.splice(this.selectedDice.indexOf(index), 1);
        }
        this.costs = this.computeCosts(this.newDices.filter((dice, index) => this.isDiceSelected(index)));
        this.isCostError = false;
        if (this.costs >= this.getPlayer().dollar) {
            this.snackbarService.openError(`Not enough money: ${this.getPlayer().dollar}$ - ${this.costs}$ = ${this.getPlayer().dollar - this.costs}$`);
            this.isCostError = true;
        }
    }
    isDiceSelected(index) {
        return this.selectedDice.includes(index);
    }
    computeCosts(dices) {
        if (!dices.length)
            return 1;
        return dices.length - 1;
    }
    getNameWaitingFor() {
        return this.lobby.users
            .filter(user => this.lobby.game.waitingFor.includes(user.id))
            .map(user => user.name);
    }
    getOtherPlayers() {
        return this.lobby.game.players.filter(player => player.userId !== this.authenticationService.getIdFromToken() && this.lobby.users.map(user => user.id).includes(player.userId));
    }
    getUserOfPlayer(player) {
        return this.lobby.users.find(user => user.id === player.userId);
    }
    getStatusOfPlayer(player) {
        let isWaitingHim = this.isWaitingForPlayer(player);
        if (isWaitingHim) {
            return this.getGameStatus();
        }
        return "";
    }
    isWaitingForPlayer(player) {
        return this.lobby.game.waitingFor.includes(player.userId);
    }
    nonHiddenDices(dices) {
        return dices.filter(dice => !dice.hidden);
    }
};
GameComponent.ctorParameters = () => [
    { type: src_app_services_authentication_service__WEBPACK_IMPORTED_MODULE_8__["AuthenticationService"] },
    { type: _lobby_socket_service__WEBPACK_IMPORTED_MODULE_11__["LobbySocketService"] },
    { type: src_app_shared_snackbar_snackbar_service__WEBPACK_IMPORTED_MODULE_9__["SnackbarService"] },
    { type: _angular_material_dialog__WEBPACK_IMPORTED_MODULE_4__["MatDialog"] }
];
GameComponent.propDecorators = {
