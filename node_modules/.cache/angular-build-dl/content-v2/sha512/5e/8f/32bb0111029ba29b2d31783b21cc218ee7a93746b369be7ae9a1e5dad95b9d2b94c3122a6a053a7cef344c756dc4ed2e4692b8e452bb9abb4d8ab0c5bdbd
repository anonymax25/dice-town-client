/* harmony import */ var src_app_app_module__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! src/app/app.module */ "ZAI4");



let ChatSocketService = class ChatSocketService {
    constructor(chatSocket) {
        this.chatSocket = chatSocket;
    }
    connect() {
        return this.chatSocket.fromEvent('connect');
    }
    joinRoom(roomId) {
        this.chatSocket.emit('joinRoom', roomId);
    }
    joinedRoom() {
        return this.chatSocket.fromEvent('joinedRoom');
    }
    leaveRoom(roomId) {
        this.chatSocket.emit('leaveRoom', roomId);
    }
    leftRoom() {
        return this.chatSocket.fromEvent('leftRoom');
    }
    recieveMessage() {
        return this.chatSocket.fromEvent('recieveMessage');
    }
    sendMessage(message) {
        this.chatSocket.emit("chatToRoom", message);
    }
};
ChatSocketService.ctorParameters = () => [
    { type: src_app_app_module__WEBPACK_IMPORTED_MODULE_2__["ChatSocket"] }
