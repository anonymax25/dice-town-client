let AuthGuardService = class AuthGuardService {
    constructor(authenticationService, titleService, router, dialog) {
        this.authenticationService = authenticationService;
        this.titleService = titleService;
        this.router = router;
        this.dialog = dialog;
    }
    canActivate(next, state) {
        if (!this.authenticationService.isLogged()) {
            this.login().subscribe(loginSuccess => {
                if (loginSuccess && next.routeConfig.path) {
                    this.router.navigate([next.routeConfig.path]);
                }
                return false;
            });
        }
        else {
            this.titleService.setTitle(`${src_environments_environment__WEBPACK_IMPORTED_MODULE_5__["environment"].title} : ${this.authenticationService.getUserFromToken().name}`);
            return true;
        }
    }
    login() {
        const loginDialog = this.dialog.open(_components_auth_login_login_component__WEBPACK_IMPORTED_MODULE_6__["LoginComponent"], {
            height: '350px',
            width: '500px',
        });
        return loginDialog.afterClosed();
    }
};
AuthGuardService.ctorParameters = () => [
    { type: _services_authentication_service__WEBPACK_IMPORTED_MODULE_7__["AuthenticationService"] },
