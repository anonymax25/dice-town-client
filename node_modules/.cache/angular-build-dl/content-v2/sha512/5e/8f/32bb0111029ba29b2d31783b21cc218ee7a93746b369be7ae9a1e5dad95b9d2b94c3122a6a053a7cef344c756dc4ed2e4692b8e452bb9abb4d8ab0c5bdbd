        this.authenticationService = authenticationService;
        this.snackBar = snackBar;
        this.dialog = dialog;
        this.hide = true;
        this.emailCtrl = formBuilder.control('', _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required);
        this.passwordCtrl = formBuilder.control('', _angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required);
        this.loginForm = formBuilder.group({
            email: this.emailCtrl,
            password: this.passwordCtrl,
        });
    }
    onSubmit() {
