let SnackbarComponent = class SnackbarComponent {
    constructor(data, ref) {
        this.data = data;
        this.ref = ref;
        this.message = '';
        this.closeButtonLabel = 'Fermer';
        this.message = this.uppercaseFirstLetter(data.message.replace(/(\n)/g, '<br>'));
        this.closeButtonLabel = this.uppercaseFirstLetter(data.closeButtonLabel || 'Fermer');
    }
    close() {
        this.ref.dismiss();
    }
    uppercaseFirstLetter(message) {
        return message[0].toUpperCase() + message.substring(1);
    }
};
